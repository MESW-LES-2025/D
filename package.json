{
  "name": "test",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev:next": "next dev --turbopack",
    "dev": "run-p db-server:file dev:*",
    "build": "next build",
    "build:local": "run-p db-server:memory build:* --race",
    "start": "next start",
    "build-stats": "cross-env ANALYZE=true npm run build",
    "clean": "rimraf .next out coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "check:types": "tsc --noEmit --pretty",
    "test": "vitest run --coverage",
    "test:unit": "vitest --project=unit",
    "test:integration": "vitest --project=integration",
    "test:e2e": "playwright test",
    "db-server:file": "pglite-server --db=local.db",
    "db-server:memory": "pglite-server",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@t3-oss/env-nextjs": "^0.13.8",
    "@tabler/icons-react": "^3.35.0",
    "@tanstack/react-table": "^8.21.3",
    "better-auth": "^1.3.32",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.44.5",
    "framer-motion": "^12.23.24",
    "next": "^16.0.0",
    "next-themes": "^0.4.6",
    "pg": "^8.16.3",
    "react": "^19.2.0",
    "react-day-picker": "^9.11.3",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.63.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.4.1",
    "@better-auth/cli": "^1.3.32",
    "@electric-sql/pglite-socket": "^0.0.15",
    "@eslint-react/eslint-plugin": "^1.53.1",
    "@faker-js/faker": "^10.0.0",
    "@next/bundle-analyzer": "^15.5.4",
    "@next/eslint-plugin-next": "^15.5.4",
    "@playwright/test": "^1.55.1",
    "@tailwindcss/postcss": "^4.1.14",
    "@types/node": "^24.6.1",
    "@types/pg": "^8.15.5",
    "@types/react": "^19.2.0",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/browser": "^4.0.14",
    "@vitest/coverage-v8": "^4.0.14",
    "@vitest/ui": "^4.0.14",
    "baseline-browser-mapping": "^2.8.32",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "cross-env": "^10.1.0",
    "drizzle-kit": "^0.31.5",
    "eslint": "^9.36.0",
    "eslint-plugin-format": "^1.0.2",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-playwright": "^2.2.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.23",
    "eslint-plugin-tailwindcss": "^4.0.0-beta.0",
    "jsdom": "^27.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "rimraf": "^6.0.1",
    "semantic-release": "^24.2.9",
    "tailwindcss": "^4.1.14",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.14",
    "vitest-browser-react": "^1.0.1"
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "staging",
        "prerelease": "beta"
      }
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits"
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  }
}
